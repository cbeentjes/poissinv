Cell[TextData[{
 "Summation terms for larger values of \[Lambda]: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", ">", "4"}], TraditionalForm]],ExpressionUUID->
  "5d35e5e0-9c08-4911-8c40-8bd4b78bee33"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "\[LessEqual]", " ", 
    RowBox[{"F", "(", 
     RowBox[{"10", ",", "\[Lambda]"}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"18ca6242-6cbf-416f-92e0-6678bb06b275"]
}], "Subsubsection",
 CellChangeTimes->{{3.764097071618919*^9, 3.764097087983881*^9}, {
  3.764098600288246*^9, 3.76409860391031*^9}, {3.8148750324417477`*^9, 
